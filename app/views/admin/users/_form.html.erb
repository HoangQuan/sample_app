<%= form_for @user do |f| %>
  <%= render "application/errors", object: f.object %>
  <table class="table table-striped table-bordered">
    <tr>
      <td>Name: <%= f.text_field :name%> </td>
    </tr>
    <tr>
      <td>Admin: <%= f.check_box :admin %></td>
    </tr>
    <tr>
      <td><%= f.submit "submit", class: "btn btn-primary"%></td>
    </tr>
  </table>
<% end %>
